{
    "rewrites": [
      {
        "source": "^/Contatos/(adicionar|deletar|editar)/?",
        "destination": "/api/contatos/:1"
      },
      {
        "source": "^/Tarefas/(adicionar|deletar|editar|visualizar)/?",
        "destination": "/api/tarefas/:1"
      },
      {
        "source": "^/index\\.php$",
        "destination": "/api/index"
      },
      {
        "source": "^/(.*)$",
        "destination": "/$1"
      }
    ]
  }
  